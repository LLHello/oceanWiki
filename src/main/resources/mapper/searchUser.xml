<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gec.oceanbioproject.mapper.UserMapper">
    <select id="getUserById" resultType="com.gec.oceanbioproject.entity.User">
        select * from user where id = #{id}
    </select>
    <select id="getAllUsers" resultType="com.gec.oceanbioproject.entity.User">
        select * from user
    </select>
</mapper>
